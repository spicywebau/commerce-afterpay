{
	"name": "spicyweb/commerce-afterpay",
	"description": "Afterpay gateway for craft commerce",
	"type": "craft-plugin",
	"version": "1.0.0",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"spicy afterpay"
	],
	"support": {
		"docs": "https://github.com/spicywebau/commerce-afterpay/blob/main/README.md",
		"issues": "https://github.com/spicywebau/commerce-afterpay/issues"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Spicy Web",
			"homepage": "https://github.com/spicywebau"
		}
	],
	"require": {
		"craftcms/cms": "^3.4.0",
		"craftcms/commerce": "^3.0.0",
		"craftcms/commerce-omnipay": "^3.0.0",
		"omnipay/common": "^3.0.3",
		"afterpay-global/afterpay-sdk-php": "^1.1"
	},
	"autoload": {
		"psr-4": {
			"spicyweb\\spicyafterpay\\": "src/"
		}
	},
	"extra": {
		"name": "Afterpay",
		"handle": "spicy-afterpay",
		"developer": "Spicy Web",
		"developerUrl": "https://github.com/spicywebau",
		"documentationUrl": "https://github.com/spicywebau/commerce-afterpay/blob/main/README.md",
		"changelogUrl": "https://github.com/spicywebau/commerce-afterpay/blob/main/CHANGELOG.md",
		"components": {
			"spicyAfterpayService": "spicyweb\\spicyafterpay\\services\\SpicyAfterpayService"
		},
		"class": "spicyweb\\spicyafterpay\\SpicyAfterpay"
	}
}
